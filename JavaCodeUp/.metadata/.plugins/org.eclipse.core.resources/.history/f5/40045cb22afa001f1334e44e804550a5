import java.util.Scanner;

public class Ans46Calc4 {

	public static void main(String[] args) {
		// 정수 3개 입력받아 합과 평균 출력하기
		Scanner sc = new Scanner(System.in);
		long a = sc.nextLong();
		long b = sc.nextLong();
		
		System.out.println(a+b);
		System.out.println(a-b);
		System.out.println(a*b);
		System.out.println(a/b);
		System.out.println(a%b);
		float c = (float)a;
		float d = (float)b;
		System.out.printf("%.2f", c/d);
	}

}
